In [47]:
from IPython.display import display,HTML
import jieba
def word_match(word,title):
    result = (word in title)
    return result
    
keyword = "中国 and (越南 or 老人)"
keyword_ = " ".join(jieba.cut(keyword))

for title in title_list:
    tempList = keyword_.split()
    for i in range(len(tempList)):
        if tempList[i] != " " and tempList[i] != 'and' and tempList[i] != "or" and tempList[i] != '(' and tempList[i]!=')':
            tempList[i] = str(word_match(tempList[i],title))
    matchresult = eval(' '.join(tempList))
    if matchresult:
        for word in keyword_.split():
            if word != ' ' and word != 'and' and word!="or" and word!=")" and word!='(':
                title = title.replace(word,'<span style="color:red">{}</span>'.format(word))
        display(HTML(title))
  

        
>中国小伙娶越南富家美女
>输完越南、缅甸竟还有得输！中国足球这回突破新下限
>蒙古国棚户区一老人向往中国
